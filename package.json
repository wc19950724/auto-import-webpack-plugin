{
  "name": "auto-import-webpack-plugin",
  "version": "0.1.11-beta.0",
  "description": "自动生成按需引入组件库",
  "main": "plugin.js",
  "module": "plugin.mjs",
  "types": "plugin.d.ts",
  "bin": {
    "auto-import": "cli.js"
  },
  "exports": {
    ".": {
      "types": "./plugin.d.ts",
      "require": "./plugin.js",
      "import": "./plugin.mjs"
    }
  },
  "scripts": {
    "start": "esno src/cli/index.ts",
    "build": "esno scripts/build.ts",
    "fix": "pnpm prettier && tsc && pnpm lint",
    "lint": "eslint **/*.{js,cjs,mjs,ts,cts,mts} --fix",
    "prettier": "prettier --ignore-path .eslintignore **/*.{js,cjs,mjs,ts,cts,mts} --write",
    "release": "release-cli"
  },
  "keywords": [
    "auto-import",
    "webpack",
    "plugin"
  ],
  "author": "948533798@qq.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wc19950724/auto-import-webpack-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/wc19950724/auto-import-webpack-plugin/issues"
  },
  "homepage": "https://github.com/wc19950724/auto-import-webpack-plugin#readme",
  "devDependencies": {
    "@types/node": "^20.2.3",
    "@typescript-eslint/parser": "^5.59.7",
    "arg": "^5.0.2",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "esno": "^0.16.3",
    "ghooks": "^2.0.4",
    "ignore": "^5.2.4",
    "lint-staged": "^13.2.2",
    "picocolors": "^1.0.0",
    "release-base-cli": "0.0.6-beta.5",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,ts,cts,mts}": [
      "pnpm fix"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "npx lint-staged --quiet --concurrent false"
    }
  },
  "dependencies": {
    "prettier": "^2.8.8",
    "webpack": "^5.83.1"
  },
  "peerDependencies": {
    "webpack": ">=5"
  }
}
